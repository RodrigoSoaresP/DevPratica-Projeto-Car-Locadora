@model CarLocadora.Modelo.VeiculoModel

@{
    ViewData["Title"] = "Edit";
}

<h4>Editar Veiculo</h4>

<br />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Edit">
            <input asp-for="DataInclusao" type="hidden">
			<input asp-for="DataAlteracao" type="hidden">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <partial name="_Formulario" />
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
